{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2017",
		"outDir": "out",
		"lib": [
			"es2017"
		],
		"sourceMap": true,
		"rootDir": ".",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"downlevelIteration": true
	},
	"exclude": [
		"node_modules",
		"binary_modules",
		"src/remote",
		".vscode-test"
	]
}
